	<queryString>
		<![CDATA[select NozzleShift_ShiftDate,NozzleShift_ShiftNo,NozzleShift_NozzleId,
(select Product_Name from tb_Product where Product_Num=NozzleShift_ProductNum) as ProductName, NozzleShift_StartVol,NozzleShift_EndVol,(NozzleShift_EndVol-NozzleShift_StartVol) as TotalVol,
(select sum(TransItem_PayVol) from tb_TransItem where TransItem_ShiftNo=NozzleShift_ShiftNo and TransItem_ShiftDate=NozzleShift_ShiftDate and TransItem_NozzleNo=NozzleShift_NozzleId and TransItem_TransDate between $P{StartTime} and $P{EndTime}) as TotalPayVol,
(select sum(TransItem_PayVol) from tb_TransItem where TransItem_ShiftNo=NozzleShift_ShiftNo and TransItem_ShiftDate=NozzleShift_ShiftDate and (TransItem_Tend='08' or TransItem_Tend='09') and TransItem_NozzleNo=NozzleShift_NozzleId and TransItem_TransDate between $P{StartTime} and $P{EndTime}) as RechargeVol,
(select sum(TransItem_PayVol) from tb_TransItem where TransItem_ShiftNo=NozzleShift_ShiftNo and TransItem_ShiftDate=NozzleShift_ShiftDate and TransItem_Tend='07' and TransItem_NozzleNo=NozzleShift_NozzleId and TransItem_TransDate  between $P{StartTime} and $P{EndTime}) as OneselfCardVol,
(select sum(TransCard_PayVol) from tb_TransCard where TransCard_ShiftNo=NozzleShift_ShiftNo and TransCard_ShiftDate=NozzleShift_ShiftDate and (TransCard_TransType='02' or TransCard_TransType='11') and TransCard_NozzleNo=NozzleShift_NozzleId and TransCard_TxCreateTime  between $P{StartTime} and $P{EndTime}) as CustomerCardPrepayVol,
(select sum(TransCard_PayVol) from tb_TransCard where TransCard_ShiftNo=NozzleShift_ShiftNo and TransCard_ShiftDate=NozzleShift_ShiftDate and (TransCard_TransType='05' or TransCard_TransType='10') and TransCard_NozzleNo=NozzleShift_NozzleId and TransCard_TxCreateTime  between $P{StartTime} and $P{EndTime}) as CustomerCardPayVol,
PreferentialCard.PreferentialVol as PreferentialVol,PreferentialCard.DiscountAmt as PreferentialDiscountAmt ,PreferentialCard.PaidinAmount as PreferentialPaidinAmount,Money.MoneyVol,Money.TotalMoney ,
GovernmentCard.GovernmentVol,GovernmentCard.GovernmentAmount,Other.OtherVol,Other.OtherAmount,Bank.BankVol,Bank.BankAmount,HeXinTong.HeXinTongVol,HeXinTong.HeXinTongAmount,Shift_StartTime,Shift_EndTime
from tb_NozzleShift
left join
tb_Shift
on Shift_ShiftDate=NozzleShift_ShiftDate and Shift_ShiftNo=NozzleShift_ShiftNo
left join
(select sum(TransCard_PayVol) as PreferentialVol,sum(TransCard_DiscountAmt) as DiscountAmt,sum(TransCard_PaidinAmount) as PaidinAmount,TransCard_ShiftDate,TransCard_NozzleNo,TransCard_ShiftNo from tb_TransCard
where TransCard_TransType='06' and TransCard_TxCreateTime  between $P{StartTime} and $P{EndTime} group by TransCard_NozzleNo,TransCard_ShiftDate,TransCard_ShiftNo)PreferentialCard
on PreferentialCard.TransCard_ShiftDate=NozzleShift_ShiftDate and PreferentialCard.TransCard_NozzleNo=NozzleShift_NozzleId
and PreferentialCard.TransCard_ShiftNo=NozzleShift_ShiftNo
left join
(select sum(TransItem_PayVol) as MoneyVol,sum(TransItem_PayMoney) as TotalMoney,TransItem_ShiftNo,TransItem_NozzleNo,TransItem_ShiftDate from tb_TransItem
where TransItem_Tend='01'and TransItem_TransDate  between $P{StartTime} and $P{EndTime} group by TransItem_NozzleNo,TransItem_ShiftDate,TransItem_ShiftNo)Money
on NozzleShift_NozzleId= Money.TransItem_NozzleNo and NozzleShift_ShiftNo=Money.TransItem_ShiftNo and Money.TransItem_ShiftDate=NozzleShift_ShiftDate
left join
(select sum(TransItem_PayVol) as GovernmentVol,sum(TransItem_PayMoney) as GovernmentAmount,TransItem_ShiftNo,TransItem_NozzleNo,TransItem_ShiftDate from tb_TransItem
where TransItem_Tend='12' and TransItem_TransDate  between $P{StartTime} and $P{EndTime}  group by TransItem_NozzleNo,TransItem_ShiftDate,TransItem_ShiftNo)GovernmentCard
on GovernmentCard.TransItem_ShiftDate=NozzleShift_ShiftDate  and GovernmentCard.TransItem_NozzleNo=NozzleShift_NozzleId
and GovernmentCard.TransItem_ShiftNo=NozzleShift_ShiftNo
left join
(select sum(TransItem_PayVol) as OtherVol,sum(TransItem_PayMoney) as OtherAmount,TransItem_ShiftNo,TransItem_NozzleNo,TransItem_ShiftDate from tb_TransItem
where (TransItem_Tend>12 or TransItem_Tend=04 or TransItem_Tend=03) and TransItem_TransDate  between $P{StartTime} and $P{EndTime}  group by TransItem_NozzleNo,TransItem_ShiftDate,TransItem_ShiftNo)Other
on Other.TransItem_ShiftDate=NozzleShift_ShiftDate  and Other.TransItem_NozzleNo=NozzleShift_NozzleId
and Other.TransItem_ShiftNo=NozzleShift_ShiftNo
left join
(select sum(TransItem_PayVol) as BankVol,sum(TransItem_PayMoney) as BankAmount,TransItem_ShiftNo,TransItem_NozzleNo,TransItem_ShiftDate from tb_TransItem
where TransItem_Tend=03 and TransItem_TransDate between $P{StartTime} and $P{EndTime} group by TransItem_NozzleNo,TransItem_ShiftDate,TransItem_ShiftNo)Bank
on Bank.TransItem_ShiftDate=NozzleShift_ShiftDate  and Bank.TransItem_NozzleNo=NozzleShift_NozzleId
and Bank.TransItem_ShiftNo=NozzleShift_ShiftNo
left join
(select sum(TransItem_PayVol) as HeXinTongVol,sum(TransItem_PayMoney) as HeXinTongAmount,TransItem_ShiftNo,TransItem_NozzleNo,TransItem_ShiftDate from tb_TransItem
where TransItem_Tend=13 and TransItem_TransDate between $P{StartTime} and $P{EndTime}  group by TransItem_NozzleNo,TransItem_ShiftDate,TransItem_ShiftNo)HeXinTong
on HeXinTong.TransItem_ShiftDate=NozzleShift_ShiftDate  and HeXinTong.TransItem_NozzleNo=NozzleShift_NozzleId
and HeXinTong.TransItem_ShiftNo=NozzleShift_ShiftNo
where NozzleShift_ShiftDate=$P{ShiftDate} and  NozzleShift_ShiftNo=$P{ShiftNo} and Shift_StartTime=$P{StartTime}  and Shift_EndTime=$P{EndTime} order by NozzleShift_NozzleId,ProductName]]>
	</queryString>
